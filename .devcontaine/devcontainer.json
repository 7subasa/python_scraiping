// VSCodeのDev containerでpyを実行するための拡張機能をインストール
{
  "name": "python3_scraiping",
  "build": {
    "context": ".",
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
      },
      "extensions": ["ms-python.python"]
    }
  }
}
